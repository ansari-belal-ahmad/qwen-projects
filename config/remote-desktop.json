{
  "server": {
    "host": "0.0.0.0",
    "http_port": 3000,
    "ws_port": 8765,
    "metrics_port": 9090,
    "max_connections": 10,
    "connection_timeout": 30,
    "http_port_fallback_start": 3001,
    "http_port_fallback_end": 3010,
    "ws_port_fallback_start": 8766,
    "ws_port_fallback_end": 8775,
    "metrics_port_fallback_start": 9091,
    "metrics_port_fallback_end": 9100
  },
  "performance": {
    "max_fps": 30,
    "jpeg_quality": 75,
    "compression_level": 6,
    "frame_queue_size": 3,
    "mouse_throttle_ms": 16,
    "enable_h264": false,
    "downscale_threshold": 1920
  },
  "security": {
    "enable_ssl": false,
    "ssl_cert_path": null,
    "ssl_key_path": null,
    "encryption_key": null,
    "auth_required": false,
    "auth_token": null,
    "allowed_ips": [],
    "block_end_key": true
  },
  "features": {
    "enable_audio": false,
    "enable_clipboard": true,
    "enable_file_transfer": true,
    "enable_session_recording": false,
    "enable_multi_monitor": true,
    "enable_auto_click": true,
    "enable_keyboard_shortcuts": true
  },
  "logging": {
    "log_level": "INFO",
    "log_file": "remote-desktop.log",
    "log_max_size": 10,
    "log_backup_count": 5,
    "enable_access_log": true,
    "enable_error_log": true
  }
}